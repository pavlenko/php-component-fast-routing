{
  "config": {
    "sort-packages": true
  },
  "name": "pe/component-fast-routing",
  "description": "Fast routing component, based on PSR-7 http messages and compiled routes",
  "keywords": ["cmf", "routing"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pavlenko Eugeniy",
      "email": "pavlenko.obs@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "pe/component-cache-file": "^1.0",
    "psr/http-message-implementation": "~1.0",
    "psr/log-implementation": "^1.0"
  },
  "require-dev": {
    "monolog/monolog": "^1.23",
    "phpunit/phpunit": "~5.0",
    "symfony/routing": "^3.3",
    "symfony/var-dumper": "^3.3",
    "zendframework/zend-diactoros": "^1.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.sunnyct.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PE\\Component\\Routing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PETest\\Component\\Routing\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
